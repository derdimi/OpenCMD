[HKEY_CLASSES_ROOT\Directory\shell\CMDLocal]
@="Open Command Window here"
"Icon"="cmd.exe"
"NoWorkingDirectory"=""

[HKEY_CLASSES_ROOT\Directory\shell\CMDLocal\command]
@="cmd.exe /s /k pushd \"%V\""





